{
  "hosting": [
    {
      "public": "dist",
      "target": "prod",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "public": "dist",
      "target": "dev",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "public": "dist",
      "target": "qa",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/viajero",
          "destination": "/traveler.html"
        }
      ]
    }
  ]
}
